[ssh-iptables]
enabled  = true
filter   = sshd
action   = iptables[name=SSH, port=ssh, protocol=tcp]
#logpath  = /var/log/sshd.log
backend = systemd
bantime=600
findtime=600
maxretry = 5

[ssh-ddos]
enabled = true
port    = ssh,sftp
filter  = sshd-ddos
backend = systemd
bantime=600
findtime=600
maxretry = 5


[nginx-http-auth]
port    = http,https
logpath = /var/log/nginx/error.log
